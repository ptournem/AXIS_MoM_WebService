    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://titan.be/axis-csrm/interoperabilitymodel/ontology/0.3#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://titan.be/axis-csrm/interoperabilitymodel/ontology/0.3">
  <owl:Ontology rdf:about="">
    <rdfs:label xml:lang="en">AXIS-CSRM Interoperability model</rdfs:label>
    <dc:description xml:lang="en">This ontology defines the concepts for AXIS-CSRM interoperability model. 
    It introduces the key concepts Autonomous Resource Domain (ARE), InterOperability Window (IOW) and P-Autonomous eXchange Entity (P-AXE).</dc:description>
    <dc:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Roger Roberts</dc:contributor>
    <dc:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Guy Mar√©chal</dc:contributor>
    <dc:contributor rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Guillaume Rachez</dc:contributor>
    <dc:date rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >2015-07-06</dc:date>
    <owl:imports rdf:resource="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl"/>
    <dc:language xml:lang="en">English</dc:language>
  </owl:Ontology>
  <owl:Class rdf:ID="ResolvingService">
    <rdfs:label xml:lang="en">Resolving service</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ConfigurationManagementService"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="AXEProfile">
    <rdfs:label xml:lang="en">AXE Profile</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="ClientARD">
    <rdfs:label xml:lang="en">Client Autonomous Resource Domain</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="ARD"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#ConfigurationManagementService">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#ARD"/>
    </rdfs:subClassOf>
    <rdfs:label xml:lang="en">Configuration management service</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="AXE">
    <rdfs:label xml:lang="en">Autonomous eXchange Entity</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="IOW">
    <rdfs:label xml:lang="en">Interoperability Wicket</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="IdentificationService">
    <rdfs:subClassOf rdf:resource="#ConfigurationManagementService"/>
    <rdfs:label xml:lang="en">Identification service</rdfs:label>
  </owl:Class>
  <owl:Class rdf:about="#ARD">
    <rdfs:label xml:lang="en">Autonomous Resource Domain</rdfs:label>
  </owl:Class>
